{
  "name": "generator-gulped",
  "version": "0.3.1",
  "description": "Yeoman generator for Gulped template",
  "homepage": "https://github.com/mrDinckleman/generator-gulped",
  "author": {
    "name": "Sergey Kovalchuk",
    "email": "admin@vitam.in.ua",
    "url": "https://github.com/mrDinckleman"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "gulp",
    "template",
    "yeoman-generator"
  ],
  "devDependencies": {
    "atob": ">=2.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.5.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "merge": ">=1.2.1",
    "nsp": "^3.2.1",
    "prettier": "^1.15.3",
    "randomatic": ">=3.0.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "dependencies": {
    "gulped": "github:mrDinckleman/gulped",
    "yeoman-generator": "^3.2.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "mrDinckleman/generator-gulped",
  "license": "MIT"
}
