{
  "name": "generator-gulped",
  "version": "0.1.4",
  "description": "Yeoman generator for Gulped template",
  "homepage": "https://github.com/mrDinckleman/generator-gulped",
  "author": {
    "name": "Sergey Kovalchuk",
    "email": "admin@vitam.in.ua",
    "url": "https://github.com/mrDinckleman"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["gulp", "template", "yeoman-generator"],
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "gulped": "github:mrDinckleman/gulped",
    "yeoman-generator": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "mrDinckleman/generator-gulped",
  "license": "MIT"
}
