{
  "name": "generator-gulped",
  "version": "0.3.5",
  "description": "Yeoman generator for Gulped template",
  "homepage": "https://github.com/mrDinckleman/generator-gulped",
  "author": {
    "name": "Sergey Kovalchuk",
    "email": "admin@vitam.in.ua",
    "url": "https://github.com/mrDinckleman"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "gulp",
    "template",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.6.0"
  },
  "dependencies": {
    "gulped": "github:mrDinckleman/gulped",
    "yeoman-generator": "^4.10.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "mrDinckleman/generator-gulped",
  "license": "MIT"
}
